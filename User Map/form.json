{
    "elements": [
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Image",
                    "image": "data:image/png;base64,",
                    "link": true,
                    "onClick": "echo 'https://sum.asmium.de';"
                },
                {
                    "type": "Label",
                    "label": "\nSymcon User Map\n ",
                    "link": false,
                    "bold": true
                }
            ]
        },
        {
            "type": "Label",
            "label": "The module offers the option of adding each Symcon user's own location marker to the user map directly from the console."
        },
        {
            "type": "ExpansionPanel",
            "caption": "User data ...",
            "expanded": true,
            "items": [
                {
                    "type": "ValidationTextBox",
                    "name": "Name",
                    "caption": "User name (forum)",
                    "validate": "^[a-zA-Z0-9\\._-]{3,16}$",
                    "width": "300px"
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "SelectLocation",
                            "name": "Coords",
                            "caption": "Location"
                        },
                        {
                            "type": "Button",
                            "name": "btnCopy",
                            "caption": "Copy",
                            "onClick": "IPS_RequestAction($id,'copy', true);",
                            "visible": true
                        }
                    ]
                },
                {
                    "type": "List",
                    "name": "Links",
                    "caption": "Links (optional)",
                    "add": true,
                    "delete": true,
                    "changeOrder": true,
                    "rowCount": 5,
                    "columns": [
                        {
                            "name": "Type",
                            "caption": "Type:",
                            "width": "150px",
                            "save": true,
                            "add": "Website",
                            "edit": {
                                "type": "Select",
                                "options": [
                                    {
                                        "caption": "Website",
                                        "value": "Website"
                                    },
                                    {
                                        "caption": "GitHub",
                                        "value": "GitHub"
                                    },
                                    {
                                        "caption": "Facebook",
                                        "value": "Facebook"
                                    },
                                    {
                                        "caption": "LinkedIn",
                                        "value": "LinkedIn"
                                    },
                                    {
                                        "caption": "Twitter",
                                        "value": "Twitter"
                                    },
                                    {
                                        "caption": "Xing",
                                        "value": "Xing"
                                    }
                                ]
                            }
                        },
                        {
                            "name": "Link",
                            "caption": "Link:",
                            "save": true,
                            "width": "auto",
                            "add": "",
                            "edit": {
                                "type": "ValidationTextBox",
                                "validate": "^(https?://)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([/\\w \\.-]*)*/?$"
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "actions": [
        {
            "type": "Label",
            "name": "lblManage",
            "caption": "Manage your marker via the buttons ...",
            "visible": true
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Button",
                    "name": "btnRegister",
                    "caption": "Register",
                    "onClick": "IPS_RequestAction($id,'usermap', 'register');",
                    "visible": true,
                    "enabled": false
                },
                {
                    "type": "Button",
                    "name": "btnUpdate",
                    "caption": "Update",
                    "onClick": "IPS_RequestAction($id,'usermap', 'update');",
                    "visible": true,
                    "enabled": false
                },
                {
                    "type": "Button",
                    "name": "btnDelete",
                    "caption": "Delete",
                    "onClick": "IPS_RequestAction($id,'usermap', 'delete');",
                    "visible": true,
                    "enabled": false
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Source code, donation and licence ...",
            "items": [
                {
                    "type": "Label",
                    "caption": "The software is free of charge for non-commercial use, I would appreciate a donation if you like the module.",
                    "bold": true
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Image",
                            "image": "data:image/svg+xml;base64,",
                            "onClick": "echo 'https://wilkware.github.io/';"
                        },
                        {
                            "type": "Image",
                            "image": "data:image/svg+xml;base64,",
                            "onClick": "echo 'https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=8816166';"
                        },
                        {
                            "type": "Image",
                            "image": "data:image/svg+xml;base64,",
                            "onClick": "echo 'https://creativecommons.org/licenses/by-nc-sa/4.0/';"
                        }
                    ]
                }
            ]
        }
    ],
    "status": [
        {
            "code": 101,
            "icon": "inactive",
            "caption": "Creating instance."
        },
        {
            "code": 102,
            "icon": "active",
            "caption": "Instance active."
        },
        {
            "code": 104,
            "icon": "inactive",
            "caption": "Instance inactive."
        },
        {
            "code": 201,
            "icon": "inactive",
            "caption": "Please provide all necessary data!"
        }
    ]
}